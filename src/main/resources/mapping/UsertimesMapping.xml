<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shark.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.shark.entity.Usertimes">
        <result column="id" jdbcType="INTEGER" property="id" />
        <result column="username" jdbcType="VARCHAR" property="username" />
        <result column="event_ts" jdbcType="timestamp" property="event_ts" />
        <result column="insert_ts" jdbcType="timestamp" property="insert_ts" />
        <result column="daytimes" jdbcType="INTEGER" property="daytimes" />
        <result column="flag" jdbcType="VARCHAR" property="flag" />
    </resultMap>

    <select id="SelectById" resultType="com.shark.entity.Usertimes">
        select * from Uusertimes where id = #{id}
    </select>

</mapper>
